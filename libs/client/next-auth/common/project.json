{
  "name": "next-auth-common",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/client/next-auth/common/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/client/next-auth//common",
        "tsConfig": "libs/client/next-auth/common/tsconfig.lib.json",
        "packageJson": "libs/client/next-auth/common/package.json",
        "main": "libs/client/next-auth/common/src/index.ts",
        "assets": ["libs/client/next-auth/common/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/client/next-auth/common/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/client/next-auth/common/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
